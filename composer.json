{
    "name": "airxyxy/jwt",
	"description": "基于lcobucci/jwt修改",
    "license": [
        "BSD-3-Clause"
    ],
    "type": "library",
    "keywords": [
        "JWT",
        "JWS"
    ],
	"authors": [
        {
            "name": "Airxyxy",
            "email": "807460184@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "lcobucci/clock": "^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.20",
        "lcobucci/coding-standard": "^6.0",
        "mikey179/vfsstream": "^1.6",
        "phpbench/phpbench": "^0.17",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Airxyxy\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Airxyxy\\JWT\\": [
                "test/_keys",
                "test/unit",
                "test/performance"
            ],
            "Airxyxy\\JWT\\FunctionalTests\\": "test/functional"
        }
    },
	"config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
